{
    "name": "skuola/pdf-text-parser",
    "description": "Library to parse XML resulting from pdftotext",
    "keywords": ["pdf", "pdftotext", "text"],
    "type": "library",
    "require": {
        "php": "^7.0",
        "symfony/css-selector": "^2.7|^3.4",
        "symfony/dom-crawler": "^2.7|^3.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.8",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": { "Skuola\\PdfTextParser\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "Skuola\\PdfTextParser\\Test\\": "tests" }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "garakkio@gmail.com"
        }
    ]
}
